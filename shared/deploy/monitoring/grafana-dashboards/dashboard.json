{
  "dashboard": {
    "title": "NSReady Platform Dashboard",
    "tags": ["nsready", "telemetry"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Ingest Rate (events/sec)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ingest_events_total[5m])",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Queue Depth",
        "type": "graph",
        "targets": [
          {
            "expr": "ingest_queue_depth",
            "legendFormat": "Queue Depth"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "P95 Latency (ms)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ingest_events_total[5m]))",
            "legendFormat": "P95 Latency"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ingest_errors_total[5m]) / rate(ingest_events_total[5m])",
            "legendFormat": "Error Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Database Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "db_latency_seconds",
            "legendFormat": "DB Latency"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "System Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "system_uptime_seconds",
            "legendFormat": "Uptime"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  }
}


